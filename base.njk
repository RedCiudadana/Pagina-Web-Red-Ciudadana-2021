          $(function () {
            var $win = $(window);
            // definir mediente $pos la altura en píxeles desde el borde superior de la ventana del navegador y el elemento
            var $pos = 50;
            $win.scroll(function () {
               if ($win.scrollTop() >= $pos)
                    $('#menu').addClass('sticky');
               else {
                    $('#menu').removeClass('sticky');          
                    
